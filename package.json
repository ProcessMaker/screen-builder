  },
  "browserslist": [
    "defaults",
    "> 0.2% and not dead"
  ],
  "engines": {
    "npm": ">=8",
    "node": ">=16 <18"
  },
  "nyc": {
    "include": [
      "src/**"
    ],
    "extension": [
      ".js",
      ".vue"
    ]
  }
}
