{
  "name": "@processmaker/vue-form-builder",
  "version": "0.9.0",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "build-bundle": "vue-cli-service build --target lib --name vue-form-builder ./src/components/index.js",
    "lint": "vue-cli-service lint",
    "test": "vue-cli-service test:unit"
  },
  "main": "./dist/vue-form-builder.common.js",
  "files": [
    "dist/*",
    "src/*",
    "public/*",
    "*.json",
    "*.js"
  ],
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.6.1",
    "@processmaker/vue-form-elements": "^0.3.0",
    "bootstrap": "^4.1.3",
    "css-tree": "^1.0.0-alpha.29",
    "expr-eval": "^1.2.2",
    "monaco-editor-webpack-plugin": "^1.7.0",
    "mustache": "^3.0.1",
    "node-sass": "^4.9.1",
    "sass-loader": "^7.0.3",
    "source-map": "^0.7.3",
    "v-calendar": "^0.9.7",
    "validatorjs": "^3.14.2",
    "vue": "^2.5.16",
    "vue-deepset": "^0.6.3",
    "vue-monaco": "^0.3.1",
    "vue-multiselect": "^2.1.3",
    "vue-uniq-ids": "^1.0.0",
    "vue-upload-component": "^2.8.14",
    "vuedraggable": "^2.16.0",
    "vuetable-2": "^1.7.5"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.0.0-rc.9",
    "@vue/cli-plugin-eslint": "^3.0.0-rc.9",
    "@vue/cli-plugin-unit-jest": "^3.0.5",
    "@vue/cli-service": "^3.0.0-rc.9",
    "@vue/test-utils": "^1.0.0-beta.20",
    "babel-core": "7.0.0-bridge.0",
    "bootstrap-vue": "^2.0.0-rc.11",
    "vue-json-pretty": "^1.4.0",
    "vue-template-compiler": "^2.5.16"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "jest": {
    "moduleFileExtensions": [
      "js",
      "jsx",
      "json",
      "vue"
    ],
    "transform": {
      "^.+\\.vue$": "vue-jest",
      ".+\\.(css|styl|less|sass|scss|svg|png|jpg|ttf|woff|woff2)$": "jest-transform-stub",
      "^.+\\.jsx?$": "babel-jest"
    },
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1"
    },
    "snapshotSerializers": [
      "jest-serializer-vue"
    ],
    "testMatch": [
      "**/tests/unit/**/*.spec.(js|jsx|ts|tsx)|**/__tests__/*.(js|jsx|ts|tsx)"
    ],
    "testURL": "http://localhost/"
  }
}
