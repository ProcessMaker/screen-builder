{
  "name": "@processmaker/screen-builder",
  "version": "2.62.1",
  "scripts": {
    "dev": "vite",
    "serve": "vue-cli-service serve",
    "build": "cross-env INSTRUMENT_BUILD=true VITE_COVERAGE=false vite build",
    "build-visualizer": "vite-bundle-visualizer",
    "preview": "vite preview",
    "test:unit": "vue-cli-service test:unit",
    "lint": "vue-cli-service lint",
    "build-bundle": "vue-cli-service build --target lib --name vue-form-builder ./src/components/index.js",
    "lint-fix": "vue-cli-service lint",
    "open-cypress": "TZ=UTC nyc cypress open",
    "run-cypress": "TZ=UTC nyc cypress run",
    "test": "vue-cli-service test:unit",
    "test:ci": "start-server-and-test 'vite dev' http://localhost:5173/ 'nyc cypress run --headless --browser chrome --spec $(cat /tmp/tests-to-run)'"
  },
  "main": "./dist/vue-form-builder.umd.js",
  "module": "./dist/vue-form-builder.es.js",
  "exports": {
    ".": {
      "import": {
        "import": "./dist/vue-form-builder.es.js",
        "require": "./dist/vue-form-builder.umd.js"
      }
    }
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "@chantouchsek/validatorjs": "^1.1.5",
    "axios-extensions": "^2.0.3",
    "lodash-es": "^4.17.21",
    "lru-cache": "^7.14.0",
    "moment": "^2.29.1",
