    "monaco-editor": "^0.34.0",
    "scrollparent": "^2.0.1",
    "vue-monaco": "^0.3.2",
    "vue-simple-uploader": "^0.7.4",
    "vue-the-mask": "^0.11.1",
    "vuelidate": "^0.7.5"
  },
  "devDependencies": {
    "@4tw/cypress-drag-drop": "^2.2.1",
